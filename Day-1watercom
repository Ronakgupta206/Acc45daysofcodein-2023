#include <stdio.h>

int main() {
    int t; // Number of test cases
    scanf("%d", &t);

    while (t--) {
        int x; // Amount of water Chef drank today
        scanf("%d", &x);

        if (x >= 2000) {
            printf("YES\n");
        } else {
            printf("NO\n");
        }
    }

    return 0;
}
