#include <stdio.h>

int main() {
    int dividend, divisor, remainder;

    // Input from the user
    printf("Enter the dividend: ");
    scanf("%d", &dividend);
    printf("Enter the divisor: ");
    scanf("%d", &divisor);

    // Calculate the remainder
    remainder = dividend % divisor;

    // Display the result
    printf("The remainder is: %d\n", remainder);

    return 0;
}
